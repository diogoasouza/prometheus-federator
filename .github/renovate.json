{
  "extends": [
    "github>rancher/renovate-config#release",
    "group:allNonMajor"
  ],
  "baseBranches": [
    "main",
    "release/v3.x",
    "release/v2.x",
    "release/v1.x",
    "release/v0.x"
  ],
  "packageRules": [
    {
      "groupName": "GitHub Workflow Actions",
      "groupSlug": "gha-deps",
      "matchManagers": [
        "github-actions"
      ]
    },
    {
      "groupName": "Docker File Deps",
      "groupSlug": "docker-bumps",
      "matchManagers": [
        "dockerfile"
      ]
    }
  ]
}
